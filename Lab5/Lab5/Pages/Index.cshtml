@page
@model IndexModel
@{
    ViewData["Title"] = "Домашня сторінка";
}

<h1>Список клієнтів, які зробили більше 1 дзвінка</h1>

<table class="table">
    <tr>
        <th>Ім'я</th>
        <th>Прізвище</th>
        <th>Міста</th>
    </tr>
    @foreach (var client in Model.ClientAndCities)
    {
        <tr>
            <td>@client.FirstName</td>
            <td>@client.LastName</td>
            <td>@client.Cities</td>
        </tr>
    }
</table>
